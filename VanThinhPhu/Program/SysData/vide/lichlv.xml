<?xml version="1.0" standalone="yes"?>
<NewDataSet>
  <xs:schema id="NewDataSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
    <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:MainDataTable="lichlv" msdata:UseCurrentLocale="true">
      <xs:complexType>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element name="lichlv">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="idvide" type="xs:string" minOccurs="0" />
                <xs:element name="iditerm" type="xs:string" minOccurs="0" />
                <xs:element name="headerV" type="xs:string" minOccurs="0" />
                <xs:element name="headerE" type="xs:string" minOccurs="0" />
                <xs:element name="exe" type="xs:string" minOccurs="0" />
                <xs:element name="image" type="xs:string" minOccurs="0" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:choice>
      </xs:complexType>
    </xs:element>
  </xs:schema>
  <lichlv>
    <idvide>lichlv</idvide>
    <iditerm>01</iditerm>
    <headerV>Sửa</headerV>
    <headerE>Edit</headerE>
    <exe>lnkcv.exe#NKCV &lt;!edit&gt; stt_rec:$[stt_rec]</exe>
    <image />
  </lichlv>
  <lichlv>
    <idvide>lichlv</idvide>
    <iditerm>02</iditerm>
    <headerV>Thêm mới</headerV>
    <headerE>Add</headerE>
    <exe>lnkcv.exe#NKCV &lt;!add&gt;</exe>
    <image />
  </lichlv>
  <lichlv>
    <idvide>lichlv</idvide>
    <iditerm>03</iditerm>
    <headerV>Xóa</headerV>
    <headerE>Delete</headerE>
    <exe>lnkcv.exe#NKCV &lt;!delete&gt; stt_rec:$[stt_rec]</exe>
    <image />
  </lichlv>
  <lichlv>
    <idvide>lichlv</idvide>
    <iditerm>04</iditerm>
    <headerV>Xem thêm</headerV>
    <headerE>View all</headerE>
    <exe>lnkcv.exe#NKCV &lt;!view&gt;</exe>
    <image />
  </lichlv>
</NewDataSet>